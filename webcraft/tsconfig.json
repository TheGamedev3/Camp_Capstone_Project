{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@MongooseSys": ["./src/lib/models/mongooseSys/Mongoose.ts"],
      "@Chemicals": ["./src/lib/models/DB_Objects/Collection.ts"],
      "@Req": ["./src/components/RequestType/index.tsx"],
      "@SiteEnv": ["./tests/framework/SiteEnv.ts"],
      "@types/*": ["types/*"],
      "@Gameplay/*": ["./src/Gameplay/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
